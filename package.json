{
  "name": "colorgy",
  "version": "0.0.1",
  "description": "Colorgy Hybrid App",
  "private": true,
  "main": "app/app.js",
  "reapp": "0.8",
  "scripts": {
    "start": "reapp-run",
    "build": "./bin/build $1",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint app ; exit 0"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "async": "^1.4.2",
    "babel-core": "^5.8.22",
    "classnames": "^2.1.3",
    "client-oauth2": "^1.0.0",
    "es6-promise": "^3.0.2",
    "fbjs": "^0.4.0",
    "fixed-data-table": "^0.4.6",
    "isomorphic-fetch": "^2.1.1",
    "material-design-icons": "^2.0.0",
    "material-ui": "^0.11.0",
    "node-sass": "^3.2.0",
    "qs": "^5.0.0",
    "react": "^0.13.3",
    "react-async": "^2.1.0",
    "react-flipcard": "^0.1.2",
    "react-redux": "^0.9.0",
    "react-router": "^0.13.3",
    "react-router-component": "^0.26.0",
    "react-select": "^0.6.7",
    "react-swipe": "^2.3.0",
    "react-tap-event-plugin": "^0.1.7",
    "react-tappable": "^0.6.0",
    "reapp": "^0.8.28",
    "redux": "^1.0.1",
    "redux-actions": "^0.7.0",
    "redux-localstorage": "^0.4.0",
    "redux-logger": "^1.0.6",
    "redux-thunk": "^0.1.0",
    "reselect": "0.0.2",
    "sass-loader": "^2.0.1",
    "swipe-js-iso": "^2.0.1",
    "url-pattern": "^0.10.0"
  },
  "devDependencies": {
    "eslint": "^1.4.3",
    "redux-devtools": "^1.1.0",
    "webpack": "^1.12.9"
  },
  "eslintConfig": {
    "ecmaFeatures": {
      "modules": true,
      "jsx": true,
      "experimentalObjectRestSpread": true
    },
    "env": {
      "browser": true,
      "node": true,
      "es6": true
    },
    "rules": {
      "no-cond-assign": 2,
      "no-dupe-args": 2,
      "no-dupe-keys": 2,
      "no-duplicate-case": 2,
      "no-empty": 2,
      "no-extra-boolean-cast": 2,
      "no-extra-parens": 1,
      "no-extra-semi": 2,
      "no-func-assign": 2,
      "no-sparse-arrays": 2,
      "no-unreachable": 1,
      "use-isnan": 2,
      "valid-jsdoc": 1,
      "valid-typeof": 2,
      "curly": 1,
      "complexity": 1,
      "brace-style": [
        1,
        "1tbs"
      ],
      "key-spacing": 2,
      "semi": [
        2,
        "always"
      ],
      "object-curly-spacing": [
        2,
        "always"
      ],
      "arrow-spacing": 2,
      "arrow-parens": [
        2,
        "always"
      ],
      "strict": 0
    }
  }
}
